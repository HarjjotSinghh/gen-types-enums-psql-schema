{
  "name": "gen-types-enums-psql-schema",
  "version": "1.3.1",
  "description": "A CLI tool to automatically generate TypeScript types and enums from any PostgreSQL DB schema",
  "main": "./dist/index.js",
  "type": "module",
  "bin": {
    "gen-types-enums-psql-schema": "./dist/index.js"
  },
  "scripts": {
    "gen-types-enums-psql-schema": "bun run ./dist/index.js",
    "build": "tsc --project tsconfig.json"
  },
  "keywords": [
    "postgresql",
    "typescript",
    "types",
    "enums",
    "drizzle",
    "cli",
    "database",
    "schema",
    "codegen"
  ],
  "author": "Harjot Singh Rana <me@harjotrana.com>",
  "license": "MIT",
  "homepage": "https://github.com/harjjotsinghh/gen-types-enums-psql-schema#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harjjotsinghh/gen-types-enums-psql-schema.git"
  },
  "bugs": {
    "url": "https://github.com/harjjotsinghh/gen-types-enums-psql-schema/issues"
  },
  "files": [
    "dist",
    "index.ts",
    "utils.ts",
    "db.config.ts",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/node": "^24.1.0",
    "ansi-colors": "^4.1.3",
    "dotenv": "^17.2.0",
    "ora": "^8.2.0"
  },
  "peerDependencies": {
    "drizzle-kit": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "drizzle-kit": {
      "optional": false
    }
  }
}